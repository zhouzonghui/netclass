  </filter-mapping>
  
  <servlet>
    <servlet-name>Logout</servlet-name>
    <servlet-class>org.graduate.web.controller.Logout</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>org.graduate.web.controller.Login</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddClass</servlet-name>
    <servlet-class>org.graduate.web.controller.AddClass</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DeleteClassUI</servlet-name>
    <servlet-class>org.graduate.web.controller.DeleteClassUI</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DeleteClass</servlet-name>
