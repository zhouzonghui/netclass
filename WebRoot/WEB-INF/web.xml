  </servlet>
  <servlet>
    <servlet-name>ScoreMrgUI</servlet-name>
    <servlet-class>org.graduate.web.controller.ScoreMrgUI</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UpdateScore</servlet-name>
    <servlet-class>org.graduate.web.controller.UpdateScore</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddNotice</servlet-name>
    <servlet-class>org.graduate.web.controller.AddNotice</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ListNotice</servlet-name>
    <servlet-class>org.graduate.web.controller.ListNotice</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DeleteNotice</servlet-name>
    <servlet-class>org.graduate.web.controller.DeleteNotice</servlet-class>
  </servlet>










  <servlet-mapping>
    <servlet-name>Logout</servlet-name>
    <url-pattern>/servlet/Logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/servlet/Login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddClass</servlet-name>
    <url-pattern>/servlet/AddClass</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteClassUI</servlet-name>
    <url-pattern>/servlet/DeleteClassUI</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteClass</servlet-name>
    <url-pattern>/servlet/DeleteClass</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ScoreMrgUI</servlet-name>
    <url-pattern>/servlet/ScoreMrgUI</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UpdateScore</servlet-name>
    <url-pattern>/servlet/UpdateScore</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddNotice</servlet-name>
    <url-pattern>/servlet/AddNotice</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ListNotice</servlet-name>
    <url-pattern>/servlet/ListNotice</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteNotice</servlet-name>
    <url-pattern>/servlet/DeleteNotice</url-pattern>
  </servlet-mapping>
